					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"var schema = {\r",
							"    \"type\" : \"object\",\r",
							"    \"properties\": \r",
							"    {\r",
							"\"page\":\t{ \"type\": \"integer\"},\r",
							"\"per_page\":\t{ \"type\": \"integer\"},\r",
							"\"total\"\t:\t{ \"type\": \"integer\"},\r",
							"\"total_pages\"\t:\t{ \"type\": \"integer\"},\r",
							"\"data\": {\"type\": \"array\",\r",
							"    \"items\":{\r",
							"        \"type\" : \"object\", \r",
							"        \"properties\" : {\r",
							"    \"id\"\t:\t{ \"type\": \"integer\"},\r",
							"    \"email\"\t:\t{ \"type\": \"string\"},\r",
							"    \"first_name\"\t:\t{ \"type\": \"string\"},\r",
							"    \"last_name\"\t:\t{ \"type\": \"string\"},\r",
							"    \"avatar\"\t:\t{ \"type\": \"string\"},\r",
							"    }}\r",
							"}\r",
							"}\r",
							"};\r",
							"\r",
							"\r",
							"var jsonData = pm.response.json()\r",
							"\r",
							"pm.test('Schema is valid', function () {\r",
							"    pm.expect(tv4.validate(jsonData, schema)).to.be.true;\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
